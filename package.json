{
  "name": "nucleus-player",
  "productName": "Nucleus Player",
  "version": "1.0.0",
  "description": "An electron wrapper for Google Play Music",
  "main": "src/index.js",
  "scripts": {
    "build": "gulp build",
    "build-packages": "npm run build-darwin && npm run build-win32 && npm run build-win64",
    "build-darwin": "electron-builder \"dist/Nucleus Player-darwin-x64/Nucleus Player.app\" --platform=osx --out=\"dist/built/osx\" --config=builder.json",
    "build-win32": "electron-builder \"dist/Nucleus Player-win32-ia32\" --platform=win --out=\"dist/built/win\" --config=builder.json",
    "build-win64": "electron-builder \"dist/Nucleus Player-win32-x64\" --platform=win --out=\"dist/built/win64\" --config=builder.json",
    "lint": "eslint ./src/",
    "package": "gulp package",
    "package-darwin": "gulp package-darwin",
    "package-win": "gulp package-win",
    "test": "npm run lint",
    "start": "electron .",
    "watch": "npm run build && gulp watch"
  },
  "author": "Samuel Attard",
  "license": "ISC",
  "dependencies": {
    "electron-prebuilt": "^0.36.3",
    "lodash": "^4.0.0",
    "materialize-css": "^0.97.5"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "electron-builder": "^2.6.0",
    "electron-packager": "^5.2.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "gmusic-mini-player.js": "^1.0.1",
    "gmusic-theme.js": "^1.0.8",
    "gmusic.js": "^4.0.3",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-clean": "^0.3.1",
    "gulp-concat": "^2.6.0",
    "gulp-cssmin": "^0.1.7",
    "gulp-electron": "0.0.10",
    "gulp-less": "^3.0.5",
    "gulp-rename": "^1.2.2"
  }
}
